<SerialDevice name="IRB1410">
    <Frame name="Robot_Base" refframe="Robot_LOC" type="Movable">
        <RPY> 0 0 0 </RPY> <Pos> 0 0 0</Pos> 
        <Property name="ObjType"  desc="Object Type"> OBJ_TYPE_ROBOT </Property>
        <Property name="RobotType" desc="robot type such as the robot has parallelogram structure">PARALLELOGRAM_ROBOT</Property>
		<Property name="Joint2MaxLimit" desc="the min and max angle of joint2">-70,70</Property>
		<Property name="Joint3MaxLimit" desc="the min and max angle of joint3">-135,140</Property>
		<Property name="PostProcess" desc=" lua">ABB.lua</Property>
    </Frame>


    <DHJoint name="Joint1" alpha="0" a="0" d="475" offset="0" type="craig"></DHJoint>

    <DHJoint name="Joint2" alpha="-90" a="150" d="0" offset="90" type="craig"></DHJoint>

    <DHJoint name="Joint3" alpha="0" a="-600" d="0" offset="0" type="craig"></DHJoint>

    <DHJoint name="Joint4" alpha="90" a="-120" d="720" offset="0" type="craig"></DHJoint>

    <DHJoint name="Joint5"  alpha="-90" a="0" d="0" offset="0" type="craig"></DHJoint>

    <DHJoint name="Joint6" alpha="90" a="0" d="0" offset="0" type="craig"></DHJoint>

    <Frame name="ToolMount" refframe="Joint6" >
        <RPY> 0 0 0 </RPY> <Pos> 0 0 85 </Pos> 
    </Frame>

    <PosLimit refjoint="Joint1" min="-170" max="170" />   <!--数据不准确，参考robotstudio-->
    <PosLimit refjoint="Joint2" min="-70" max="65" />
    <PosLimit refjoint="Joint3" min="-65" max="70" />
